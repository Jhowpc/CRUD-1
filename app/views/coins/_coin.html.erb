<div id="<%= dom_id coin %>">
 
  <tr>
    <th>
      <%= coin.description %>
    </th>

    <th>
      <%= coin.acronym %>
    </th>

    <th>
      <%= image_tag coin.url_image, width:25, height:25 %> 
    </th>
    
    <th>
      <%= link_to t('links.show'), coin %>
    <th>

    <th>
      <%= link_to t('links.edit'), edit_coin_path(coin) %>
    </th>


  </tr>
 
</div>
